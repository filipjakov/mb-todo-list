{
	"name": "mb-todo-list",
	"version": "0.0.1",
	"description": "Todo list",
	"scripts": {
		"analyze": "ANALYZE=true npm run build",
		"dev": "NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"start": "next start",
		"lint": "npm run type-check && next lint",
		"type-check": "tsc --project tsconfig.json --pretty --noEmit",
		"pre-commit": "lint-staged",
		"postinstall": "husky install",
		"test": "echo 'No tests :( Skipping...'"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:filipjakov/mb-todo-list.git"
	},
	"keywords": [],
	"author": "fbulic",
	"license": "ISC",
	"dependencies": {
		"@reach/dialog": "0.16.0",
		"classnames": "2.3.1",
		"framer-motion": "6.3.3",
		"next": "12.1.5",
		"next-plugin-preact": "3.0.6",
		"preact": "10.7.1",
		"preact-render-to-string": "5.1.21",
		"react": "npm:@preact/compat@17.0.3",
		"react-dom": "npm:@preact/compat@17.0.3"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "12.1.5",
		"@types/react": "17.0.43",
		"@typescript-eslint/eslint-plugin": "5.21.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "8.14.0",
		"eslint-config-next": "12.1.5",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "4.0.0",
		"husky": "7.0.4",
		"lint-staged": "12.4.0",
		"postcss-mixins": "9.0.2",
		"postcss-preset-env": "7.4.3",
		"prettier": "2.6.2",
		"typescript": "4.6.3"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"browserslist": [
		"defaults",
		"iOS >= 13",
		"not dead",
		"not ie 11",
		"not op_mini all",
		"not and_qq > 0",
		"not Baidu > 0",
		"not kaios > 0"
	],
	"sideEffects": false
}
